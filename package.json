	"private": false,
	"scripts": {
		"dev": "next dev",
		"build": "sh ./scripts/build.sh",
		"build-deploy": "sh ./scripts/deploy.sh",
		"start": "next start",
		"format": "eslint * --fix --no-error-on-unmatched-pattern && prettier --write *",
		"lint": "eslint --no-error-on-unmatched-pattern * && prettier --check *",
		"generate-latex": "ts-node scripts/latex > ./public/files/EmmaJCline-resume.tex",
		"check-latex": "sh ./scripts/latex/check.sh",
		"build-latex": "sh ./scripts/latex/build.sh",
		"latex": "yarn run build-latex && yarn run check-latex",
		"test": "jest",
		"test-watch": "jest --watch",
		"docz-dev": "docz dev",
		"docz-build": "sh ./scripts/docz-build.sh"
	},
	"dependencies": {
		"@mdx-js/loader": "1.6.21",
