{
	"name": "emma.adams.engineer",
	"version": "1.0.0",
	"repository": "https://github.com/emmajsadams/personal-website",
	"author": "Emma Adams <emma@adams.engineer>",
	"private": false,
	"engines": {
		"node": ">=12.x"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build && npm run docz-build",
		"start": "next start",
		"format": "prettier --check *",
		"format-fix": "prettier --write *",
		"lint": "eslint * --fix --no-error-on-unmatched-pattern",
		"lint-fix": "eslint --no-error-on-unmatched-pattern *",
		"generate-latex": "ts-node scripts/latex > ./public/files/EmmaAdams_Resume.tex",
		"check-latex": "sh ./scripts/latex/check.sh",
		"build-latex": "sh ./scripts/latex/build.sh",
		"latex": "npm run build-latex && npm run check-latex",
		"test": "jest",
		"test-watch": "jest --watch",
		"test-coverage": "jest --coverage",
		"docz-dev": "rm -rf .docz && docz dev",
		"docz-build": "rm -rf public/docz && docz build && rm -rf public/docz/public",
		"ci-test": "npm run lint && npm run format && npm run test-coverage"
	},
	"dependencies": {
		"@mdx-js/loader": "1.6.21",
		"@next/mdx": "9.5.5",
		"next": "10.0.1",
		"next-mdx-enhanced": "5.0.0",
		"prism-react-renderer": "1.1.1",
		"react": "17.0.1",
		"react-cookie-consent": "6.2.1",
		"react-dom": "17.0.1"
	},
	"devDependencies": {
		"@mdx-js/mdx": "1.6.21",
		"@testing-library/jest-dom": "5.11.6",
		"@testing-library/react": "11.2.2",
		"@types/jest": "26.0.15",
		"@types/json-schema": "7.0.6",
		"@types/node": "14.6.4",
		"@types/react": "16.9.49",
		"@typescript-eslint/eslint-plugin": "4.4.0",
		"@typescript-eslint/parser": "4.4.0",
		"autoprefixer": "10.0.2",
		"browserslist": "4.14.7",
		"docz": "2.3.1",
		"eslint": "7.11.0",
		"eslint-config-prettier": "6.12.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-markdown": "2.0.0-rc.1",
		"eslint-plugin-mdx": "1.8.2",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "7.21.5",
		"feed": "4.2.1",
		"front-matter": "4.0.2",
		"gatsby-plugin-postcss": "3.4.0",
		"globby": "11.0.1",
		"husky": "^4.3.8",
		"jest": "26.6.3",
		"postcss": "8.1.7",
		"postcss-cli": "8.3.1",
		"postcss-flexbugs-fixes": "5.0.1",
		"postcss-preset-env": "6.7.0",
		"prettier": "2.2.1",
		"prettier-plugin-organize-imports": "1.1.1",
		"prettier-plugin-sh": "0.6.0",
		"react-axe": "3.5.4",
		"tailwindcss": "2.0.1",
		"ts-jest": "26.4.4",
		"ts-node": "9.0.0",
		"typescript": "4.0.3"
	}
}
