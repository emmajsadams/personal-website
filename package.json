{
	"name": "emma.adams.engineer",
	"version": "1.0.1",
	"repository": "https://github.com/emmajsadams/personal-website",
	"author": "Emma Adams <emma@adams.engineer>",
	"private": false,
	"engines": {
		"node": "16.14.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "npm run lint && npm run format && npm run test-coverage && next build",
		"start": "next start",
		"format": "prettier --check *",
		"format-fix": "prettier --write *",
		"lint": "eslint * --no-error-on-unmatched-pattern",
		"lint-fix": "eslint * --fix --no-error-on-unmatched-pattern",
		"generate-latex": "ts-node scripts/latex > ./public/files/EmmaAdams_Resume.tex",
		"check-latex": "sh ./scripts/latex/check.sh",
		"build-latex": "sh ./scripts/latex/build.sh",
		"latex": "npm run build-latex && npm run check-latex",
		"test": "jest",
		"test-watch": "jest --watch",
		"test-coverage": "jest --coverage",
		"docz-dev": "rm -rf .docz && docz dev",
		"docz-build": "rm -rf public/docz && docz build && rm -rf public/docz/public"
	},
	"dependencies": {
		"next": "12.1.6",
		"next-mdx-remote": "4.0.3",
		"prism-react-renderer": "1.3.3",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"@mdx-js/mdx": "1.6.21",
		"@next/eslint-plugin-next": "12.1.6",
		"@testing-library/jest-dom": "5.11.6",
		"@testing-library/react": "11.2.2",
		"@types/jest": "28.1.0",
		"@types/json-schema": "7.0.6",
		"@types/node": "17.0.38",
		"@types/react": "17.0.2",
		"@typescript-eslint/eslint-plugin": "5.27.0",
		"@typescript-eslint/parser": "5.27.0",
		"autoprefixer": "10.4.7",
		"browserslist": "4.20.3",
		"docz": "2.4.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-markdown": "2.2.1",
		"eslint-plugin-mdx": "1.17.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.30.0",
		"feed": "4.2.1",
		"front-matter": "4.0.2",
		"gatsby-plugin-postcss": "5.15.0",
		"globby": "11.0.1",
		"husky": "4.3.8",
		"jest": "28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"postcss": "8.4.14",
		"postcss-cli": "9.1.0",
		"postcss-flexbugs-fixes": "5.0.2",
		"postcss-preset-env": "7.7.0",
		"prettier": "2.6.2",
		"prettier-plugin-organize-imports": "2.3.4",
		"prettier-plugin-sh": "0.11.0",
		"react-axe": "3.5.4",
		"tailwindcss": "2.0.1",
		"ts-jest": "28.0.4",
		"ts-node": "10.8.0",
		"typescript": "4.7.2"
	}
}
